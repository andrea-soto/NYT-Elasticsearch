<script type="text/javascript">
/* resizerScript.js */
require(['foundation/main'], function() {
    define('nytg-resizer', ["jquery/nyt","underscore/nyt","foundation/views/base-view","foundation/views/page-manager"],function(e,s,t,i){var n=t.extend({sizes:{v1:{180:".g-show-xsmall",300:".g-show-small",460:".g-show-smallplus",600:".g-show-submedium",720:".g-show-medium",945:".g-show-large",1050:".g-show-xlarge"},v2:{180:".g-show-xsmall",300:".g-show-small",460:".g-show-smallplus",495:".g-text-width",600:".g-show-submedium",720:".g-show-medium",945:".g-show-large",1050:".g-show-xlarge",1400:".g-show-xxlarge"}},initialize:function(t){return e("html").hasClass("g-resizer-init")?!1:(this.selector=s.values(this.sizes.v2).join(", "),s.bindAll(this,"render","selectElements"),this.selectElements(),this.pageManager.on("nyt:page-resize",this.render),this.render(),e("html").addClass("g-resizer-init"),void e(document).ready(this.selectElements))},render:function(){var t=this
    t.elements.each(function(){var i=e(this),n="v1"
    i.hasClass("g-resizer-v2")&&(n="v2")
    var l=s.map(s.keys(t.sizes[n]),Number),r=i.parent().width(),a=s.max(s.filter(l,function(e){return r>=e})),h=t.sizes[n][a]||"",o=i.hasClass(h.replace(".",""))
    i.toggle(o)})},selectElements:function(){this.elements=e(this.selector),this.elements.hide(),this.render()}})
    return new n})

    require(['nytg-resizer'], function(resizer) {
        require(['shared/interactive/instances/app-communicator'], function(AppCommunicator) {
            // when the height of the interactive is set
            AppCommunicator.triggerResize();
        });
    });
});
</script>

